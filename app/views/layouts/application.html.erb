<!DOCTYPE html>
<html>
<head>
  <title>Checkin</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="wrapper" class="container_24">
  <% if session[:user] %>
    <%= link_to 'Log out', logout_path, :method => :delete %>
    <%= link_to 'Search', search_path %>
  <% end %>
  <div id="main">
    <%= yield %>
  </div>
</div>
</body>
</html>
